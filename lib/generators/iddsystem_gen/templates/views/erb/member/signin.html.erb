<div class="container">
  <%= flash_message%>
  <h2>Sign In Members</h2>
  <div class="row">
    <div class="col-xs-6">
      <%= form_for :signin_members, url: check_signin_members_path, method: :post, html: {id: 'signin_members'} do |f| %>
        <div class="form-group">
          <%= f.label :bp_email, 'Email'%>
          <%= f.email_field :bp_email, id: 'bp_email', class: 'form-control'%>
        </div>
        <div class="form-group">
          <%= f.label :password, 'Password'%>
          <%= f.password_field :password, id: 'password', class: 'form-control'%>
        </div>
        <%= f.button :submit,class: "btn btn-default"%>
      <% end %>
    </div>
  </div>
</div>